package com.example.demo.game;

public class GameRunner {

	
	public void run() {
		GamingConsole g = new Mario();
        g.up();
        g.down();
        g.left();
        g.right();

        GamingConsole g2 = new SuperMario();
        g2.up();
        g2.down();
        g2.left();
        g2.right();
    }
	

}
